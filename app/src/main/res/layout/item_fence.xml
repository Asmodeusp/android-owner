<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:gravity="center_vertical"
    android:background="@color/white"
    android:paddingLeft="@dimen/px24dp"
    android:paddingRight="@dimen/px24dp"
    android:layout_marginBottom="@dimen/px20dp"
    android:paddingBottom="@dimen/px20dp"
    android:paddingTop="@dimen/px20dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:visibility="gone"
        android:gravity="center_vertical"
        android:text=""
        android:textSize="@dimen/px30dp"
        android:textColor="@color/color_black"
        android:id="@+id/tvfencetype"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </TextView>
    <TextView
        android:layout_marginTop="@dimen/px20dp"
        android:gravity="center_vertical"
        android:text=""
        android:textSize="@dimen/px30dp"
        android:textColor="@color/color_black"
        android:id="@+id/tvfenceName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    </TextView>



</LinearLayout>